aws cloudformation create-stack --stack-name <StackName> -- https://s3-us-west-2.amazonaws.com/idc-aws-session/HandsOnExerciseStack.json --parameters 
ParameterKey=LoadBalancerName,ParameterValue=<ElbName> ParameterKey=KeyName,ParameterValue=<KeyName> ParameterKey=ImageId,ParameterValue=<imageId>


aws cloudformation delete-stack --stack-name <Stackname>
